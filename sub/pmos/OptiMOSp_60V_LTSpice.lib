.options Thev_Induc=1

.SUBCKT P2_60_r_var dd g s0 Tj PARAMS: a=1 dVth=0 dR=0 dgfs=0 Inn=1
+Unn=1 Rmax=1 gmin=1 Rs=1 Rp=1 dC=0 Rm=1u

.PARAM b0=7.08        p0=2.97        p1=9.8u        p2=2.72u       mubet=675.88m  fbet=0
.PARAM Vth0=2.03      c=787.46m      Fm=10.94m      Fn=977.1u      al=500m        auth=1.34m
.PARAM dvx=550m       dvgs=245.07m   auth_sub=1.63m

.PARAM Rd=195m        rpara=7m       nmu=2.12       Rf=0

.PARAM lnIsj=-25.6    ndi=1.17       Rdi=10.1m      nmu2=0         n_Isj=2.23     UB=64.9
.PARAM ab=50.84m      ab2=0          UT=100m        lB=-23         ta=2n

.PARAM kbq=85.8u      Tref=298       T0=273

.PARAM f3=295.28p     f3b=5.62p

.PARAM f4=212.68p     f5=316.06p     sl=173.19p     ps1=10.28p     ps2=-9.89m     ps3=347.66p
.PARAM ps4=-215.55m   ps5=0.93p      ps6=0.14p      ps7=3.82       remp=0.8p      pc0=2.32p

.PARAM q83=128.29p    q84=-147.74m

.PARAM q80=65.5p      q81=359.68p    q82=39.09p     qs1=7.68p      qs2=36.05p     qs3=-15.55m
.PARAM f2r=92.75m

.PARAM x1={(q80-q81)/q82}       x2={q80/q82}
.PARAM y1={(f4-f5)/sl}          y2={f4/sl}
.PARAM Vmin=1.53      Vmax=2.53      dCmax=330m
.PARAM Vth={Vth0+(Vmax-Vth0)*limit(dVth,0,1)-(Vmin-Vth0)*limit(dVth,-1,0)}
.PARAM q0={b0*((1-fbet)*(T0/Tref)**mubet+fbet)*a}
.PARAM q1={(Unn-Inn*Rs-Vth0)*q0}
.PARAM q2={(Fm*SQRT(0.4)-c)*Inn*q0}
.PARAM Rlim={((q1+2*q2*Rmax-SQRT(q1**2+4*q2))/(2*q2))/(1+rpara*(Inn/a)**2)}
.PARAM dRd={Rd/a+if(dVth==0,limit(dR,0,1)*max(Rlim-Rd/a-Rs-Rp,0),0)}
.PARAM bm={c/((1/gmin-Rs)**2*Inn*a*((1-fbet)*(T0/Tref)**mubet+fbet))}
.PARAM bet={b0+(b0-bm)*if(dR==0,if(dVth==0,limit(dgfs,-1,0),0),0)}
.PARAM dC1={1+dCmax*limit(dC,0,1)}
.PARAM dC2={1+dCmax*limit(dC,0,1)}

.PARAM Cgs0={(f3*a+sqrt(a)*f3b)*dC1}

.PARAM Cds3={q83*a*dC1}
.PARAM Cds5={qs1*a*dC1}
.PARAM Cds6={(a*qs2*(1+f2r/sqrt(a)))*dC1}
.PARAM Cds8={q80*a*dC1}

.PARAM Cdg1={(a*ps1+pc0*sqrt(a))*dC2}
.PARAM Cdg2={ps3*a*dC2}
.PARAM Cdg3={(ps5*a+remp*sqrt(a)+ps6)*dC2}
.PARAM Cdg4={f4*a*dC2}
.PARAM dRdi={Rdi/a}

.FUNC Ue(g,y,w)  {(g-Vth+auth*(w-Tref)+Fm*y**Fn)}
.FUNC Ue1(g,y,w) {Ue(g,y,w)+(1+limit(Ue(g,y,w)+dvx,0,1)**2*(2*limit(Ue(g,y,w)+dvx,0,1)-3))*(dvgs+(auth_sub-auth)*(w-Tref))}

.FUNC I0(Uee,p,pp,z1) {if(Uee>pp,(Uee-c*z1)*z1,p*(pp-p)/c*exp((Uee-pp)/p))}
.FUNC Ih(Uds,T,p,Uee) {bet*((1-fbet)*(T0/T)**mubet+fbet)*I0(Uee,p,min(2*p,p+c*Uds),min(Uds,Uee/(2*c)))}
.FUNC Jh(d,g,w,y,s,x) {a*((Ih(s*y+min(d,0),w,(p0+(p1+p2*w)*w)*kbq*w,Ue1(g,y,w))+exp(min(lB+(d-UB-ab*(w-Tref))/UT,24))))}

.FUNC Idiode(Usd,Tj,Iss) {exp(min(log(Iss)+Usd/(ndi*kbq*Tj),7))-Iss}
.FUNC Idiod(Usd,Tj)      {a*Idiode(Usd,Tj,exp(min(lnIsj+(Tj/Tref-1)*1.12/(ndi*kbq*Tj),7))*(Tj/Tref)**n_Isj)}

.FUNC Pr(Vss0)       {Vss0*Vss0/Rm}
.FUNC Q01(Uds) {a*(limit(Uds,x1,x2)*(q80-q82/2*limit(Uds,x1,x2))+min(Uds-x1,0)*q81-max(x1,0)*(q80-q81)/2)}
.FUNC Q02(Udg) {a*(limit(Udg,y1,y2)*(f4-sl/2*limit(Udg,y1,y2))+min(Udg-y1,0)*f5-max(y1,0)*(f4-f5)/2)}

C_Cgs  g      s      {Cgs0}

E_Eds3 d      edep3  VALUE {if(q84==0,0,-(V(s,d)-(exp(q84*max(V(s,d),-1))-1)/min(q84,-1u)-min(V(s,d)+1,0)*exp(-q84)))}
C_Cds3 edep3  s      {Cds3}

C_Cds5 d      s      {Cds5}
E_Eds6 d      edep6  VALUE {if(qs3==0,0,-(V(s,d)-(exp(qs3*max(V(s,d),0))-1)/min(qs3,-1u)-min(V(s,d),0)))}
C_Cds6 edep6  s      {Cds6}
E_Eds8 d      edep8  VALUE {-(V(s,d)-Q01(V(s,d))/Cds8)}
C_Cds8 edep8  s      {Cds8}

E_Edg1 d    ox1 VALUE {if(ps2==0,0,-(V(g,d)-(exp(ps2*max(V(g,d),0))-1)/min(ps2,-1u)-min(V(g,d),0)))}
C_Cdg1 ox1  g   {Cdg1}
E_Edg2 d    ox2 VALUE =
+{if(ps4==0,0,-(V(g,d)-((exp(ps4*(max(V(g,d)+ps7,0)))-exp(ps4*max(ps7,0)))/min(ps4,-1u)+min(V(g,d)+max(ps7,0),max(0,-ps7)))))}
C_Cdg2 ox2  g   {Cdg2}
C_Cdg3 d    g   {Cdg3}
E_Edg4 d    ox4 VALUE {-(V(g,d)-Q02(V(g,d))/Cdg4)}
C_Cdg4 ox4  g   {Cdg4}



G_chan   d5a   s  VALUE={-Jh(V(s,d5a),V(s,g),T0+limit(V(Tj),-200,300),(SQRT(1+4*al*max(V(s,d5a),0))-1)/2/al,sgn(V(s,d5a)),0)}
Rd06     d5a  d5  1u
V_sm     d    d5  0
G_RMos   d1    d  VALUE={V(d1,d)/(Rf*dRd+(1-Rf)*dRd*((limit(V(Tj),-200,999)+T0)/Tref)**nmu)/(1+rpara*(I(V_sense)/a)**2)}
V_sense  dd   d1  0
G_Rdio   d2   d1  VALUE={V(d2,d1)/(dRdi*((limit(V(Tj),-200,999)+T0)/Tref)**nmu2)}
V_sense2 d2   d3  0

Dbody d3 s dbody
.model dbody D (BV= {UB*10},CJO ={Cds3/1000},TT ={ta},IS ={a*exp(lnIsj)} m={0.3} RS={dRdi*1m} n={ndi})


R1        g    s  1G
Rd01      d    s  500Meg
Rd02     d2    s  500Meg
Rd03     d1    d  1k


Rmet      s    s0 {Rm}


G_TH      0   Tj  VALUE =
+{(I(V_sense)-I(V_sense2))*V(d1,d)+I(V_sm)*V(d,s)+I(V_sense2)*V(d1,s)+Pr(V(s,s0))}


.ENDS

