.PARAM q83=16p        q84=-11.11m

.PARAM q80=348p       q81=50p        q82=12p        qs1=7p         qs2=25p        qs3=-2
.PARAM qs4=160p       qs5=-83.33m    f2r=0

.PARAM x1={(q80-q81)/q82}       x2={q80/q82}
